{
  "id": "com.keepersecurity.vault",
  "command": "/app/start.sh",
  "separate-locales": false,
  "runtime": "org.freedesktop.Platform",
  "runtime-version": "20.08",
  "sdk": "org.freedesktop.Sdk",
  "base": "org.electronjs.Electron2.BaseApp",
  "base-version": "20.08",
  "finish-args": [
    "--device=dri",
    "--filesystem=home",
    "--share=ipc",
    "--share=network",
    "--socket=x11"
  ],
  "modules": [
    {
      "name": "keepersecurity",
      "sources": [
        {
          "type": "file",
          "url": "https://www.keepersecurity.com/desktop_electron/Linux/repo/deb/keeperpasswordmanager_16.0.1_amd64.deb",
          "sha256": "5752793f3d3cc54b6f25dd8b049904803deb5034ff40cb7f387e4b98a9f20e60"
        },
        {
          "type": "script",
          "dest-filename": "start.sh",
          "commands": [
            "exec zypak-wrapper /app/usr/lib/keeperpasswordmanager/keeperpasswordmanager $@"
          ]
        },
        {
          "type": "file",
          "path": "com.keepersecurity.vault.desktop"
        },
        {
          "type": "file",
          "path": "com.keepersecurity.vault.png"
        },
        {
          "type": "file",
          "path": "com.keepersecurity.vault.metainfo.xml"
        }
      ],
      "buildsystem": "simple",
      "build-commands": [
        "ar x keeperpasswordmanager_16.0.1_amd64.deb",
        "tar xf data.tar.xz -C /app",
        "rm -Rf keepersecurity-16.0.1.deb control.tar.gz data.tar.xz debian-binary",
        "find /app/usr/lib/keeperpasswordmanager/resources -type d -print0 | xargs -0 chmod 755",
        "chmod 755 /app/usr/lib/keeperpasswordmanager/locales",
        "chmod 755 /app/usr/lib/keeperpasswordmanager/swiftshader",
        "chmod 755 /app/usr/lib/keeperpasswordmanager/keeperpasswordmanager",
        "mv start.sh /app"
      ],
      "post-install": [
        "install -Dm644 com.keepersecurity.vault.desktop /app/share/applications/com.keepersecurity.vault.desktop",
        "install -Dm644 com.keepersecurity.vault.png /app/share/icons/com.keepersecurity.vault.png",
        "install -Dm644 com.keepersecurity.vault.metainfo.xml /app/share/metainfo/com.keepersecurity.vault.metainfo.xml",
        "install -Dm644 com.keepersecurity.vault.desktop /app/usr/share/applications/com.keepersecurity.vault.desktop",
        "install -Dm644 com.keepersecurity.vault.png /app/usr/share/icons/com.keepersecurity.vault.png",
        "install -Dm644 com.keepersecurity.vault.metainfo.xml /app/usr/share/metainfo/com.keepersecurity.vault.metainfo.xml"
      ]
    }
  ]
}
